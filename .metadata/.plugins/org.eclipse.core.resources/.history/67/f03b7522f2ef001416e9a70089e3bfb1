
public class Leap_Yr {

	public static void main(String[] args) {

		float year=2004;
		
		if(year%100==0 && year%400==0)
		{
			System.out.println(+year+ " is a Leap Year"+year );
		}
		
		else if(year%100!=0 && year%4==0)
		{
			System.out.println( +year+ " is a Leap Year "+year );
		}
		
		else
		{
			System.out.println( +year+ " is Not a Leap Year"+year );
		}
			

	}

}
