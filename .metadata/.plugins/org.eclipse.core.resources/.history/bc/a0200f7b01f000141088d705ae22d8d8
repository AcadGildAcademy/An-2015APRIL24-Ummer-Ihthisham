
public class Vowel {

	public static void main(String[] args) {
		
		String alpha="a";
		
		if(alpha == "a"|| alpha=="A" || alpha=="e" || alpha=="E" || alpha=="i"
				|| alpha=="I" || alpha=="o" || alpha=="O" || alpha=="u" || alpha=="U")
		{
			System.out.println(" is Vowel");
		}
		
		else
		{
			System.out.println(" is not Vowel");
		}
		
	}

}
