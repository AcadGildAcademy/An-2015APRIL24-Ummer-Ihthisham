
public class Num_Chk {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int number=3;
		
		System.out.println("Entered Number : "+number);
		
		if(number%2==0)
		{
			System.out.println("The Number Is Even");
		}
		else
			System.out.println("The Number is Odd");
}
}
